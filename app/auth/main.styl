.toolBarContainer div
  width 100%
  padding 0px


.toolBarContainer button
  background-color #f8f8f8
  width 39px !important
  margin 0px

.leftbar p
  margin-top 15px
  width 85%
  margin-left auto
  margin-right auto
  color #000000

.leftbar .collapseBtn
  float right
  margin-right 10px
  cursor pointer

.leftbar .list-group
  -webkit-border-radius 0px !important
  -moz-border-radius 0px !important
  border-radius 0px !important
  margin 0
  height 88%
  overflow auto !important

.leftbar .list-group-item
  height 100px
  border-top-right-radius 0px
  border-top-left-radius 0px
  -webkit-border-radius 0px !important
  -moz-border-radius 0px !important
  border-radius 0px !important
  background-color #f8f8f8
  position relative
  -webkit-transition-duration: 0.5s;
  font-size 11px

.leftbar .list-group-item:hover
  background-color #ffffff
  cursor pointer

.leftbar .list-group-item .toolBarContainer
  position absolute
  bottom 5px
  right 0px
  width 100%
  padding 0px

.leftbar #toolIcon
  color gray
  size 9px
  margin-right 15px

.leftbar #toolIcon:hover
  color black

.leftbar .tallClass
  height 500px

.hiddenElement
  visibility hidden

.leftbar form
  padding 0px
  margin-bottom 0px
  margin-top 0px
  width 100%
  

.leftbar input
  width 100%
  margin-bottom 0px

.leftbar .suggestions
  width 100%
  overflow-y scroll
  max-height 250px

.suggestedClass
  padding 2px
  cursor pointer
  font-size 10px
  border 1px solid
  border-radius 4px
  -webkit-transition-duration: 0.2s;

.suggestedClass:hover
  background-color #8181F7

.suggestions p
  margin 0px
  padding 0px

.blockDiv
  display block

.noneDiv
  display none

.greenText
  color #0B610B

.redText
  color #8A0808

.leftbar hr
  margin 0px

